{
  "name": "tttc-light-js",
  "version": "1.0.0",
  "description": "Express.js server for turbo pipeline and report generation",
  "scripts": {
    "build": "npx babel src --out-dir dist --extensions \".tsx,.ts\"",
    "watch": "npx babel src --out-dir dist --extensions \".tsx,.ts\" --watch",
    "test": "npm run build && node dist/test.js",
    "start": "node dist/server.js",
    "format": "npx prettier . --write"
