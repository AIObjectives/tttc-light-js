    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "prettier": "3.2.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.2",
    "@google-cloud/storage": "^7.7.0",
    "axios": "^1.6.7",
